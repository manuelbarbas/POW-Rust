  "napi": {
    "name": "POW-Rust",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu",
        "aarch64-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.16.1",
    "@types/node": "^22.15.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "npx napi artifacts",
    "build": "npx napi build --platform --release",
